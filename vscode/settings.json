{
	"editor.fontSize": 14,
	"editor.rulers": [
		120
	],
	"editor.fontLigatures": true,
	"editor.tabSize": 4,
	"editor.trimAutoWhitespace": true,
	"editor.tabCompletion": "on",
	"explorer.openEditors.visible": 0,
	"window.restoreWindows": "none",
	"terminal.integrated.fontSize": 14,
	"extensions.autoUpdate": true,
	"editor.cursorBlinking": "phase",
	"go.useLanguageServer": true,
	"window.title": "${activeEditorMedium}${separator}${rootName}",
	"files.trimTrailingWhitespace": true,
	"editor.minimap.enabled": false,
	"editor.renderWhitespace": "boundary",
	"workbench.colorTheme": "Nord",
	"extensions.ignoreRecommendations": false,
	"go.coverageDecorator": {
		"type": "gutter"
	},
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"/vendor": true
	},
	"workbench.fontAliasing": "auto",
	"workbench.editor.showIcons": true,
	"explorer.confirmDragAndDrop": false,
	"search.smartCase": true,
	"files.associations": {
		"*.tmpl": "gotemplate"
	},
	"go.testTimeout": "3m",
	"breadcrumbs.enabled": true,
	"telemetry.enableTelemetry": false,
	"go.useCodeSnippetsOnFunctionSuggest": true,
	"go.formatTool": "goimports",
	"search.useGlobalIgnoreFiles": true,
	"workbench.editor.highlightModifiedTabs": true,
	"search.showLineNumbers": true,
	"go.autocompleteUnimportedPackages": true,
	"terminal.integrated.cursorBlinking": true,
	"go.coverOnSingleTest": true,
	"terminal.integrated.cursorStyle": "line",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"go.alternateTools": {
		"go-langserver": "gopls"
	},
	"window.newWindowDimensions": "maximized",
	"go.buildFlags": [
		"-v"
	],
	"go.delveConfig": {
		"debugAdapter": "dlv-dap",
	},
	"editor.fontFamily": "'Inconsolata Regular', 'Fira Code', Menlo, Monaco, 'Courier New', monospace",
	"editor.smoothScrolling": true,
	"files.exclude": {
		"yarn.lock": true
	},
	"workbench.preferredDarkColorTheme": "Nord",
	"files.insertFinalNewline": true,
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"vim.useCtrlKeys": false,
	"editor.codeActionsOnSave": {},
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"vim.handleKeys": {
		"<C-v>": false,
		"<C-c>": false,
		"<D-c>": false,
	},
	"merge-conflict.autoNavigateNextConflict.enabled": true,
	"merge-conflict.diffViewPosition": "Below",
	"[elixir]": {
		"editor.defaultFormatter": "JakeBecker.elixir-ls"
	},
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": [
				"u"
			],
			"after": [],
			"commands": [
				{
					"command": "undo",
					"args": []
				}
			]
		},
		{
			"before": [
				"<C-r>"
			],
			"after": [],
			"commands": [
				{
					"command": "redo",
					"args": []
				}
			]
		}
	],
	"workbench.colorCustomizations": {
		"[Nord]": {
			"selection.background": "#ff0000",
			"editor.selectionBackground": "#161515",
			"list.activeSelectionForeground": "#000000",
			"list.focusHighlightForeground": "#030000"
		},
		"[City Lights]": {
			"selection.background": "#ff0000",
			"editor.selectionBackground": "#161515",
			"list.activeSelectionForeground": "#000000",
			"list.focusHighlightForeground": "#030000"
		}
	},
	"[javascript]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
		"editor.formatOnSave": false
	},
	"editor.formatOnSave": true,
	"redhat.telemetry.enabled": false,
	"go.toolsManagement.autoUpdate": true,
	"terminal.integrated.tabs.enabled": true,
	"go.addTags": {
		"tags": "json,bson"
	},
	"mdb.confirmRunAll": false,
	"editor.suggest.showStatusBar": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"security.workspace.trust.untrustedFiles": "open",
	"[yaml]": {
		"editor.formatOnSave": false
	},
	"window.zoomLevel": 0,
	"editor.bracketPairColorization.enabled": true,
}
